{
  "$schema": "./.ship/schema/ship.schema.json",
  "name": "demo-agent",
  "version": "1.0.0",
  "start": {
    "port": 3000,
    "host": "0.0.0.0",
    "interactiveWeb": false,
    "interactivePort": 3001
  },
  "llm": {
    "provider": "custom",
    "model": "${LLM_MODEL}",
    "baseUrl": "${LLM_BASE_URL}",
    "apiKey": "${LLM_API_KEY}",
    "temperature": 0.7
  },
  "adapters": {
    "telegram": {
      "enabled": true,
      "botToken": "${TELEGRAM_BOT_TOKEN}"
    },
    "feishu": {
      "enabled": false,
      "domain": "https://open.feishu.cn"
    },
    "qq": {
      "enabled": false,
      "sandbox": false
    }
  }
}
